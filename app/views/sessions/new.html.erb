<center><h1> Login </h1>
<%= form_with(scope: :user, url: login_path) do |f| %> 
      
  <%= f.text_field :username %>
  <%= f.label :username %>
  <br>

  <%= f.password_field :password %>
  <%= f.label :password %><br>


  <br/>
  <%= f.submit "Login", class: "btn btn-secondary" %><br><br>
  <%= button_to 'Login Google', "/auth/google_oauth2" , class: 'nav-link' %>

<% end %></center>